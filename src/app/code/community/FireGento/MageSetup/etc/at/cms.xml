<config>
    <default>
        <magesetup>
            <pages>
                <default>
                    <magesetup_404>
                        <execute>1</execute>
                        <title>Seite nicht gefunden</title>
                        <identifier>no-route</identifier>
                        <root_template>one_column</root_template>
                        <filename>magesetup/page/404.html</filename>
                    </magesetup_404>

                    <magesetup_imprint>
                        <execute>1</execute>
                        <title>Impressum</title>
                        <identifier>imprint</identifier>
                        <root_template>one_column</root_template>
                        <filename>magesetup/page/imprint.html</filename>
                    </magesetup_imprint>

                    <magesetup_payment>
                        <execute>1</execute>
                        <title>Zahlungsarten</title>
                        <identifier>payment</identifier>
                        <root_template>one_column</root_template>
                        <filename>magesetup/page/payment.html</filename>
                    </magesetup_payment>

                    <magesetup_privacy>
                        <execute>1</execute>
                        <title>Datenschutz</title>
                        <identifier>privacy</identifier>
                        <root_template>one_column</root_template>
                        <filename>magesetup/page/privacy.html</filename>
                    </magesetup_privacy>

                    <magesetup_shipping>
                        <execute>1</execute>
                        <title>Lieferung</title>
                        <identifier>shipping</identifier>
                        <root_template>one_column</root_template>
                        <filename>magesetup/page/shipping.html</filename>
                    </magesetup_shipping>

                    <magesetup_order>
                        <execute>1</execute>
                        <title>Bestellvorgang</title>
                        <identifier>order_process</identifier>
                        <root_template>one_column</root_template>
                        <filename>magesetup/page/order_process.html</filename>
                    </magesetup_order>

                    <magesetup_business_terms>
                        <execute>1</execute>
                        <title>AGB</title>
                        <identifier>business_terms</identifier>
                        <root_template>one_column</root_template>
                        <filename>magesetup/page/business_terms.html</filename>
                    </magesetup_business_terms>

                    <magesetup_revocation>
                        <execute>1</execute>
                        <title>Widerrufsbelehrung</title>
                        <identifier>revocation</identifier>
                        <root_template>one_column</root_template>
                        <filename>magesetup/page/revocation.html</filename>
                    </magesetup_revocation>
                </default>
            </pages>

            <blocks>
                <default>
                    <gs_business_terms>
                        <execute>1</execute>
                        <title>AGB</title>
                        <identifier>gs_business_terms</identifier>
                        <filename>magesetup/block/business_terms.html</filename>
                    </gs_business_terms>

                    <gs_revocation>
                        <execute>1</execute>
                        <title>Widerrufsbelehrung</title>
                        <identifier>gs_revocation</identifier>
                        <filename>magesetup/block/revocation.html</filename>
                    </gs_revocation>

                    <gs_footerlinks>
                        <execute>1</execute>
                        <title>Footer Links</title>
                        <identifier>footer_links</identifier>
                    </gs_footerlinks>
                </default>
            </blocks>

            <footer_links>
                <default>
                    <business_terms>
                        <title>AGB</title>
                        <target>business_terms</target>
                    </business_terms>
                    <revocation>
                        <title>Widerruf</title>
                        <target>revocation</target>
                    </revocation>
                    <shipping>
                        <title>Lieferung</title>
                        <target>shipping</target>
                    </shipping>
                    <payment>
                        <title>Zahlungsarten</title>
                        <target>payment</target>
                    </payment>
                    <privacy>
                        <title>Datenschutz</title>
                        <target>privacy</target>
                    </privacy>
                    <order>
                        <title>Bestellvorgang</title>
                        <target>order</target>
                    </order>
                    <imprint>
                        <title>Impressum</title>
                        <target>imprint</target>
                    </imprint>
                </default>
            </footer_links>
        </magesetup>
    </default>
</config>
